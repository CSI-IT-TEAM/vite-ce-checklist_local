{
  "name": "vite-ce-checklist",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "electron/main.js",
  "homepage": "./",
  "author": "CE Checklist Team",
  "description": "Checklist Management System",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "electron": "electron .",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:5173 && cross-env NODE_ENV=development electron .\"",
    "electron:build": "npm run build && electron-builder",
    "electron:dist": "npm run build && electron-builder --publish=never"
  },
  "dependencies": {
    "@emotion/styled": "^11.14.1",
    "@hookform/error-message": "^2.0.1",
    "@mui/icons-material": "^7.3.5",
    "@mui/material": "^7.3.5",
    "@mui/styled-engine": "^7.3.5",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-query": "^5.90.11",
    "@yudiel/react-qr-scanner": "^2.5.0",
    "class-variance-authority": "^0.7.1",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "html5-qrcode": "^2.3.8",
    "lucide-react": "^0.555.0",
    "qr-scanner": "^1.4.2",
    "react": "^19.2.0",
    "react-datepicker": "^8.10.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.66.1",
    "react-intl": "^7.1.14",
    "react-router-dom": "^7.9.6",
    "react-select": "^5.10.2",
    "tailwind-merge": "^3.4.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-datepicker": "^6.2.0",
    "@types/react-dom": "^19.2.3",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-basic-ssl": "^2.1.0",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "sass-embedded": "^1.93.3",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "npm:rolldown-vite@7.2.5"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.2.5"
  },
  "build": {
    "appId": "com.ce.checklist",
    "productName": "CE Checklist",
    "directories": {
      "output": "dist"
    },
    "files": [
      "dist/**/*",
      "public/**/*",
      "index.html",
      "package.json",
      "electron/**/*"
    ],
    "extraFiles": [
      {
        "from": "public",
        "to": ""
      }
    ],
    "win": {
      "target": ""
    }
  }
}
